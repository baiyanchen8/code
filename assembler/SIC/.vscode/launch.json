{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) 連接",
      "type": "cppdbg",
      "request": "attach",
      "program": "輸入程式名稱，例如 ${workspaceFolder}/a.exe",
      "MIMode": "gcc",
      "miDebuggerPath": " C:/Program Files (x86)/Dev-Cpp/MinGW64/bin/gcc.exe",
      "setupCommands": [
        {
          "description": "啟用 gdb 的美化顯示",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "將反組譯碼變體設為 Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "(gdb) Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "輸入程式名稱，例如 ${workspaceFolder}/a.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/path/to/gdb",
      "setupCommands": [
        {
          "description": "啟用 gdb 的美化顯示",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "將反組譯碼變體設為 Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "c:/Users/陳柏硯/Desktop/assembler/SIC",
      "program": "c:/Users/陳柏硯/Desktop/assembler/SIC/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}